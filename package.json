{
  "private": true,
  "version": "0.0.0",
  "name": "example",
  "type": "module",
  "devDependencies": {
    "@babel/core": "*",
    "@babel/preset-env": "*",
    "@eslint/js": "^9.14.0",
    "babel-jest": "*",
    "eslint": "^9.14.0",
    "eslint-plugin-jest": "^28.9.0",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "jest-runner-eslint": "^2.2.1"
  },
  "scripts": {
    "test": "jest && node --test --test-timeout=3000"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "dependencies": {
    "@types/node": "^22.8.7",
    "eslint-plugin-jsdoc": "^50.5.0",
    "eslint-plugin-prettier": "^5.2.1",
    "node": "^22.11.0",
    "prettier": "^3.3.3"
  },
  "jest": {
    "runner": "jest-runner-eslint",
    "displayName": "lint",
    "testMatch": [
      "<rootDir>/exclude/*.js",
      "<rootDir>/src/*.js",
      "<rootDir>/src/**/*.js",
      "<rootDir>/main.js"
